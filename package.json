{
  "name": "initial_sda",
  "version": "0.0.1",
  "description": "Initial Pemex Data Input Application",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es6.js",
  "scripts": {
    "clean": "rimraf dist && rimraf dist-dev",
    "std": "standard --verbose | snazzy",
    "start": "node dist/index.js",
    "start:dev": "nodemon --watch . ./server/index.js --exec babel-node & npm run watch:client",
    "start:windows": "start nodemon --watch . ./server/index.js --exec babel-node & npm run watch:client",
    "start:prod": "nodemon --watch . ./dist/index.js & npm run watch:client & npm run watch:server",
    "watch:server": "nodemon ./server/index.js --exec babel-node",
    "start:server": "babel-node ./server/index.js",
    "start:all": "npm run start:server & npm run start:client",
    "prebuild": "npm run clean",
    "build": "npm run build:server & npm run build:client",
    "build:client": "webpack -p --progress",
    "build:server": "rollup -c rollup.server.js",
    "querykiller": "babel-node ./server/commands/query-killer.js",
    "watch:client": "webpack --progress --watch",
    "user": "babel-node ./server/auth/users.js"
  },
